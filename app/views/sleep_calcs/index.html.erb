<p id="notice"><%= notice %></p>

<h1>Sleep Calcs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sleep_calcs.each do |sleep_calc| %>
      <tr>
        <td><%= link_to 'Show', sleep_calc %></td>
        <td><%= link_to 'Edit', edit_sleep_calc_path(sleep_calc) %></td>
        <td><%= link_to 'Destroy', sleep_calc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sleep Calc', new_sleep_calc_path %>
